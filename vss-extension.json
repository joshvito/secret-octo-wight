{
    "manifestVersion": 1,
    "id": "PrGraderizer",
    "name": "Pull Request Graderizer",
    "version": "0.0.1",
    "publisher": "JoshVito",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "This extension will grade your pull requests",
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "pullrequest",
        "grader",
        "code quality",
        "peer review",
        "code review"
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "graderizer"
        },
        {
            "path": "README.md",
            "addressable": true
        },
        {
            "path": "assets/extension-icon.png",
            "addressable": true
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/joshvito/secret-octo-wight"
    },
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "graderizer"
            }
        }
    ]
}